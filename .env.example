# MongoDB password for the existing connection
MONGODB_PWD=your_mongodb_password_here

# Hypixel API key (get from https://developer.hypixel.net/)
HYPIXEL_API_KEY=your_hypixel_api_key_here

# Redis (Upstash) for caching - get from https://upstash.com/
UPSTASH_REDIS_REST_URL=your_upstash_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token_here

# Optional: Set to development for dev mode
NODE_ENV=production
